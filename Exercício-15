print('-'*15)
print('SÃ©rie Fibonacci')
print('-'*15)


n = int(input('Digite um termo:'))

termo1 = 0
termo2 = 1
print('*'*100)
print(f'{termo1} -> {termo2}',end=' ')

cont=3

while cont <= n:
    termo3 = termo1 + termo2
    print(f'->{termo3}',end=' ')
    termo1 = termo2
    termo2 = termo3
    cont +=1
    
print('.FIM')   
print('*'*100)
