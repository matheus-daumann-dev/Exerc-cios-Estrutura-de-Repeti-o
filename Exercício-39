numero_alunos = []
altura = []

for pergunta1 in range(10):
    pergunta1 = int(input('Digite seu número:'))
    pergunta2 = float(input('Digite sua altura:'))
    numero_alunos.append(pergunta1)
    altura.append(pergunta2)
   
    
codigo_alto = altura.index(max(altura))
codigo_baixo = altura.index(min(altura))     #Função index utilizada para pegar o codigo dos elemetos da lista codigos_clientes

print(f'Codigo do aluno mais alto:{numero_alunos[codigo_alto]}|Altura:{max(altura)}')
print(f'Codigo do aluno mais baixo:{numero_alunos[codigo_baixo]}|Altura:{min(altura)}')
