numero = int(input('Digite um número inteiro:'))
resultado = 0
div = []

for count in range(numero):
    if numero % (count+1) == 0:
        resultado +=1
        div.append (count+1)
               
if resultado == 2:
    print(f'O número {numero} é primo, é divisivel por {div}')
       
else:
    
    print(f'O número {numero} não é primo, é divisivel por {div}')
